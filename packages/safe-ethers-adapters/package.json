{
  "name": "@gnosis.pm/safe-ethers-adapters",
  "version": "0.1.0-alpha.1",
  "description": "Safe Ethers Adapters",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "Ethereum",
    "Gnosis",
    "Safe",
    "Ethers"
  ],
  "scripts": {
    "unbuild": "rimraf dist *.tsbuildinfo",
    "build": "tsc",
    "test": "nyc mocha -r ts-node/register tests/**/*.test.ts",
    "example:creation": "ts-node example/creation.ts",
    "example:interaction": "ts-node example/interaction.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/safe-core-sdk.git"
  },
  "author": "Gnosis (https://gnosis.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gnosis/safe-core-sdk/issues"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "homepage": "https://github.com/gnosis/safe-core-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ethers": "^5.1.4",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "src/**/!(*test).ts": [
      "yarn lint",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "@ethersproject/bignumber": "^5.0.0",
    "@ethersproject/abstract-signer": "^5.0.0",
    "@ethersproject/abstract-providers": "^5.0.0",
    "@ethersproject/properties": "^5.0.0"
  },
  "dependencies": {
    "@gnosis.pm/safe-core-sdk": "^0.1.1",
    "@gnosis.pm/safe-deployments": "^1.0.0",
    "axios": "^0.21.1"
  }
}
